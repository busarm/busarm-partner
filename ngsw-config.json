{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://cdn.busarm.com/*",
          "https://cdn.staging.busarm.com/*"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/svg/**"
        ],
        "urls": [
          "https://cdn.busarm.com/*",
          "https://cdn.staging.busarm.com/*"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://api.busarm.com",
        "https://api.staging.busarm.com"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "30d"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "https://api.busarm.com/app/trip",
        "https://api.busarm.com/app/ticket",
        "https://api.busarm.com/app/bus",
        "https://api.busarm.com/app/location",
        "https://api.busarm.com/agent/trip",
        "https://api.busarm.com/agent/ticket",
        "https://api.busarm.com/agent/bus",
        "https://api.busarm.com/agent/location",
        
        "https://api.staging.busarm.com/app/trip",
        "https://api.staging.busarm.com/app/ticket",
        "https://api.staging.busarm.com/app/bus",
        "https://api.staging.busarm.com/app/location",
        "https://api.staging.busarm.com/agent/trip",
        "https://api.staging.busarm.com/agent/ticket",
        "https://api.staging.busarm.com/agent/bus",
        "https://api.staging.busarm.com/agent/location"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1m"
      }
    }
  ]

}
